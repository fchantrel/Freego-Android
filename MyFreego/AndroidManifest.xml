<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.fchantrel.myfreego"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="fr.fchantrel.myfreego.activity.AccueilActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".activity.LoginDisplayActivity"
            android:label="@string/title_activity_login_display" />
        
        <activity
            android:name=".activity.ListViewExampleActivity"
            android:label="@string/title_activity_list_example" />
        
        <activity
            android:name=".activity.ProductsListView"
            android:label="@string/title_products_list_view" />
        
    </application>

</manifest>
